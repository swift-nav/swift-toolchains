D=$( (cd `dirname $0`/../.. >/dev/null; pwd -P) )
R=$D/sysroot

export LD_LIBRARY_PATH="${R}/usr/lib/x86_64-linux-gnu"

exec ${R}/usr/bin/arm-linux-gnueabihf-objcopy $@

